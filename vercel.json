{
  "version": 2,
  "builds": [
    { "src": "src/server.js", "use": "@vercel/node" },
    { "src": "src/swagger/*.yaml", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/swagger/(.*)", "dest": "/src/swagger/$1" },
    { "src": "/api-docs", "dest": "/src/server.js" },
    { "src": "/(.*)", "dest": "/src/server.js" }
  ]
}
